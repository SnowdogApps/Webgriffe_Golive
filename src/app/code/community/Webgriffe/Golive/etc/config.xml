<?xml version="1.0"?>
<config>
    <modules>
        <Webgriffe_Golive>
            <version>0.1.0</version>
        </Webgriffe_Golive>
    </modules>
    <global>
        <helpers>
            <webgriffe_golive>
                <class>Webgriffe_Golive_Helper</class>
            </webgriffe_golive>
        </helpers>
        <models>
            <webgriffe_golive>
                <class>Webgriffe_Golive_Model</class>
            </webgriffe_golive>
        </models>
    </global>
    <default>
        <dev>
            <log>
                <webgriffe_golive_active>1</webgriffe_golive_active>
            </log>
        </dev>
        <webgriffe_golive>
            <checkers>
                <config_general_web_unsecure_base_url>
                    <disabled>0</disabled>
                    <name><![CDATA[Unsecure Base URL]]></name>
                    <description><![CDATA[]]></description>
                    <default_severity>error</default_severity>
                    <class>webgriffe_golive/checker_config_unsecureBaseUrl</class>
                </config_general_web_unsecure_base_url>
                <config_general_web_secure_base_url>
                    <disabled>0</disabled>
                    <name><![CDATA[Secure Base URL]]></name>
                    <description><![CDATA[]]></description>
                    <default_severity>error</default_severity>
                    <class>webgriffe_golive/checker_config_secureBaseUrl</class>
                </config_general_web_secure_base_url>
                <theme_filesystem_favicon>
                    <disabled>0</disabled>
                    <name><![CDATA[Favicon]]></name>
                    <description><![CDATA[Theme Favicon on File System]]></description>
                    <default_severity>warning</default_severity>
                    <class>webgriffe_golive/checker_theme_favicon</class>
                </theme_filesystem_favicon>
                <html_head_default_title>
                    <disabled>0</disabled>
                    <name><![CDATA[HTML Head Default Title]]></name>
                    <description><![CDATA[System > Configuration > GENERAL > Design > HTML Head > Default Title]]></description>
                    <default_severity>warning</default_severity>
                    <class>webgriffe_golive/checker_config_defaultValue</class>
                    <xml_config_path><![CDATA[design/head/default_title]]></xml_config_path>
                    <default_config_value><![CDATA[Magento Commerce]]></default_config_value>
                </html_head_default_title>
                <html_head_default_description>
                    <disabled>0</disabled>
                    <name><![CDATA[HTML Head Default Description]]></name>
                    <description><![CDATA[System > Configuration > GENERAL > Design > HTML Head > Default Description]]></description>
                    <default_severity>warning</default_severity>
                    <class>webgriffe_golive/checker_config_defaultValue</class>
                    <xml_config_path><![CDATA[design/head/default_description]]></xml_config_path>
                    <default_config_value><![CDATA[Default Description]]></default_config_value>
                </html_head_default_description>
                <html_head_default_keywords>
                    <disabled>0</disabled>
                    <name><![CDATA[HTML Head Default Keywords]]></name>
                    <description><![CDATA[System > Configuration > GENERAL > Design > HTML Head > Default Keywords]]></description>
                    <default_severity>warning</default_severity>
                    <class>webgriffe_golive/checker_config_defaultValue</class>
                    <xml_config_path><![CDATA[design/head/default_keywords]]></xml_config_path>
                    <default_config_value><![CDATA[Magento, Varien, E-commerce]]></default_config_value>
                </html_head_default_keywords>
                <html_head_default_robots>
                    <disabled>0</disabled>
                    <name><![CDATA[HTML Head Default Robots]]></name>
                    <description><![CDATA[System > Configuration > GENERAL > Design > HTML Head > Default Robots]]></description>
                    <default_severity>warning</default_severity>
                    <class>webgriffe_golive/checker_config_defaultValue</class>
                    <xml_config_path><![CDATA[design/head/default_robots]]></xml_config_path>
                    <default_config_value><![CDATA[INDEX,FOLLOW]]></default_config_value>
                    <fault_operand>neq</fault_operand>
                </html_head_default_robots>
                <html_head_demo_notice>
                    <disabled>0</disabled>
                    <name><![CDATA[HTML Head Demo Notice]]></name>
                    <description><![CDATA[System > Configuration > GENERAL > Design > HTML Head > Display Demo Store Notice]]></description>
                    <default_severity>error</default_severity>
                    <class>webgriffe_golive/checker_config_defaultValue</class>
                    <xml_config_path><![CDATA[design/head/demonotice]]></xml_config_path>
                    <default_config_value><![CDATA[1]]></default_config_value>
                </html_head_demo_notice>
            </checkers>
        </webgriffe_golive>
    </default>
</config>